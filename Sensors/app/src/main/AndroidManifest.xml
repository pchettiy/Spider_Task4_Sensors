<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.praba1110.sensors" >
<uses-permission android:name="ANDROID.PERMISSION.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".accelerometer"
            android:label="@string/title_activity_accelerometer" >
        </activity>
        <activity
            android:name=".gyro"
            android:label="@string/title_activity_gyro" >
        </activity>
        <activity
            android:name=".proximity"
            android:label="@string/title_activity_proximity" >
        </activity>
    </application>

</manifest>
